<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <CCX_ROOT>$(MSBuildThisFileDirectory)..\..\..\..\</CCX_ROOT>
    <CCX_ROOT_2>$(MSBuildThisFileDirectory)..\..\..\cocos2d-x\</CCX_ROOT_2>
    <GAF_SOURCES_ROOT>$(MSBuildThisFileDirectory)..\..\Library\Sources\</GAF_SOURCES_ROOT>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="CCX_ROOT">
      <Value>$(CCX_ROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="CCX_ROOT_2">
      <Value>$(CCX_ROOT_2)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="GAF_SOURCES_ROOT">
      <Value>$(GAF_SOURCES_ROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>